<!-- patient_item.xml -->
<RelativeLayout    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="200dp"
    android:layout_margin="10dp"
    android:background="@drawable/patbg"
    android:orientation="vertical"
    android:padding="16dp">


    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="110dp"
        android:layout_height="103dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="30dp"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.505"
        app:srcCompat="@drawable/patimgfec" />

    <TextView
        android:id="@+id/textView41"
        android:layout_width="211dp"
        android:layout_height="50dp"
        android:layout_above="@+id/textView42"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="10dp"
        android:layout_toEndOf="@+id/imageView13"
        android:fontFamily="@font/lato_black"
        android:text="TextView"
        android:textSize="30dp"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView42"
        android:layout_width="206dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView43"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="14dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="8dp"
        android:layout_toEndOf="@+id/imageView13"
        android:fontFamily="@font/lato"
        android:text="TextView"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView13"
        app:layout_constraintTop_toBottomOf="@+id/textView45"
        app:layout_constraintVertical_bias="0.207" />

    <TextView
        android:id="@+id/textView43"
        android:layout_width="199dp"
        android:layout_height="25dp"
        android:layout_above="@+id/textView45"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="73dp"
        android:layout_marginBottom="6dp"
        android:layout_toEndOf="@+id/imageView13"
        android:fontFamily="@font/lato"
        android:text="TextView"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView13"
        app:layout_constraintTop_toBottomOf="@+id/textView44"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView45"
        android:layout_width="217dp"
        android:layout_height="29dp"
        android:layout_above="@+id/textView44"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/imageView13"
        android:fontFamily="@font/lato"
        android:text="TextView"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/textView42"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView13"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.286" />

    <TextView
        android:id="@+id/textView44"
        android:layout_width="159dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="3dp"
        android:layout_toEndOf="@+id/imageView13"
        android:fontFamily="@font/lato"
        android:text="TextView"
        android:textSize="20dp" />

</RelativeLayout>